{
    "name": "starknet-cairo-101",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/starknet-cairo-101",
    
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "Starkware.cairo",
          "ms-vscode.sublime-keybindings",
          "dbaeumer.vscode-eslint",
          "wesbos.theme-cobalt2",
          "CoenraadS.bracket-pair-colorizer"
        ],
        "settings": {
          "terminal.integrated.defaultProfile.linux": "zsh",
          "workbench.colorTheme": "Cobalt2",
          "dotfiles.repository": "isabelatravaglia/dotfiles",
          "dotfiles.targetPath": "~/dotfiles",
          "dotfiles.installCommand": "~/dotfiles/install.sh",
          "editor.wordWrap": "on"
        }
      }
    },
    "remoteUser": "cairo",
    // "postStartCommand": "source ~/cairo_venv/bin/activate"
    "postStartCommand": "echo 'export STARKNET_NETWORK=alpha-goerli\nexport STARKNET_WALLET=starkware.starknet.wallets.open_zeppelin.OpenZeppelinAccount' >> ~/.zshrc && npm install starknet dotenv axios"
  }
  